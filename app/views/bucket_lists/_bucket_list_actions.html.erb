<% if already_in_bucket_list(achievement) %>
	<%= link_to 'Ta bort från önskelista', remove_bucket_list_item_path(achievement.id), id: 'remove-achievement', class: 'button expand success achievement-actions' %>
<% else %>
	<%= link_to 'Lägg till i önskelista', add_bucket_list_item_path(achievement.id), id: 'add-achievement', class: 'button expand success achievement-actions' %>
<% end %>