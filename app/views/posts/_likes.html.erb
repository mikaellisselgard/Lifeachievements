<% likes.each do |like| %>
	<% if like.user.avatar.length > 0 %>
		<%= link_to image_tag(like.user.avatar, width: "50px", height: "50px"), user_path(like.user)%>
	<% else %>
		<%= link_to image_tag('avatar.png', width: "50px", height: "50px"), user_path(like.user)%>
	<% end %>
	<%= like.user.name %><br>
<% end %>