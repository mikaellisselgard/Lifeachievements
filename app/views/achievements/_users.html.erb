<% @user_ids.each do |user| %>
	<% @achievement_user = User.find(user) %>
	<% if @achievement_user.avatar.length > 0 %>
		<%= link_to image_tag(@achievement_user.avatar, width: "50px", height: "50px"), user_path(@achievement_user)%>
	<% else %>
		<%= link_to image_tag('avatar.png', width: "50px", height: "50px"), user_path(@achievement_user)%>
	<% end %>
	<%= @achievement_user.name %><br>
<% end %>
