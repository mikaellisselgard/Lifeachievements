<% current_user.follows.each do |follow| %>
	<!-- follow.followable to get the users you follow -->
	<% if follow.followable.avatar.length > 0 %>
		<%= link_to image_tag(follow.followable.avatar, width: "50px", height: "50px"), user_tip_path(follow.followable, achievement: @achievement), method: :post %>
	<% else %>
		<%= link_to image_tag('avatar.png', width: "50px", height: "50px"), user_tip_path(follow.followable, achievement: @achievement), method: :post %>
	<% end %>
	<%= follow.followable.name %><br>
<% end %>
